@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>
    <script type="text/javascript" src="../../javascript/jquery-1.7.2.js"></script>
    <script src="~/Scripts/vue/dist/vue.js"></script>
    <script src="~/Scripts/element-ui/lib/index.js"></script>
    <link href="~/Scripts/element-ui//lib/theme-chalk/index.css" rel="stylesheet" />
    <script src="~/Scripts/axios/dist/axios.js"></script>
    <script type="text/javascript"></script>
</head>
<body>
    <div id="app">
        <form method="post" action="../customiz/customizAction!findMajorKind">
            <table width="100%">
                <tr>
                    <td>
                        <el-breadcrumb separator="/">
                            <el-breadcrumb-item><a href="/Main/main">首页</a></el-breadcrumb-item>
                            <el-breadcrumb-item>人力资源</el-breadcrumb-item>
                            <el-breadcrumb-item>客户化设置</el-breadcrumb-item>
                            <el-breadcrumb-item>人力资源档案管理设置</el-breadcrumb-item>
                            <el-breadcrumb-item>职位分类设置</el-breadcrumb-item>
                        </el-breadcrumb>
                    </td>
                </tr>
                <tr>
                </tr>
            </table>
        </form>
        <template>

            <el-row>
                <el-col :span="6">
                    <el-button type="primary" size="small" v-on:click="add()">新增</el-button>
                    <el-button type="primary" size="small" v-on:click="fanhui()">返回</el-button>
                </el-col>
            </el-row>
        </template>
        <el-form ref="form" :model="form" label-width="100px">
            <el-form-item label="职位分类编号">
            </el-form-item>
            <el-form-item label="职位分类名称">
                <el-input v-model="form.Major_kind_name"></el-input>
            </el-form-item>
        </el-form>
    </div>
    <script type="text/javascript">
        var vr = new Vue({
            el: "#app",
            data() {
                return {
                    form: {
                        Major_kind_name: '',
                    }
                }
            }, methods: {
                add: function () {
                    var t = this;
                    axios.get('/KeHu/ZhiWeiINSERT', {
                        params: {
                            configs: t.form.Major_kind_name,
                        }
                    }).then(res => {
                        console.log(t.form.Major_kind_name);
                        if (res.data > 0) {
                            alert("添加成功")
                        } else {
                            alert("删除失败!")
                        }

                    }).catch(err => {
                        console.log(err);
                        console.log(12);
                    })
                }
            }
        })
    </script>
</body>
</html>