
@{
    Layout = null;
}

<!DOCTYPE html>
@using Model
@model Engage_Major_Release
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>biangen_Show</title>
    <link rel="stylesheet" href="../../css/table.css" type="text/css">
    <script type="text/javascript" src="../../javascript/comm/comm.js"></script>
    <script type="text/javascript" src="../../javascript/jquery-1.7.2.js"></script>
    <script src="~/Scripts/vue/dist/vue.js"></script>
    <script src="~/Scripts/element-ui/lib/index.js"></script>
    <link href="~/Scripts/element-ui//lib/theme-chalk/index.css" rel="stylesheet" />
    <script src="~/Scripts/axios/dist/axios.js"></script>
    <script type="text/javascript"></script>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>

    <script type="text/javascript">
        window.onload = check;
        function tick() {
            var now = new Date();
            var hours, minutes, seconds, noon;
            var intHours, intMinutes, intSeconds;
            intHours = now.getHours();
            intMinutes = now.getMinutes();
            intSeconds = now.getSeconds();
            if (intHours < 24) {
                hours = intHours + ":";
                noon = "A.M.";
            } else {
                intHours = intHours - 24;
                hours = intHours + ":";
                noon = "P.M.";
            }
            if (intMinutes < 10) {
                minutes = "0" + intMinutes + ":";
            } else {
                minutes = intMinutes + ":";
            }
            if (intSeconds < 10) {
                seconds = "0" + intSeconds + " ";
            } else {
                seconds = intSeconds + " ";
            }
            timeString = hours + minutes + seconds;
            var now = new Date();
            document.getElementById("nowTime").value = now.getFullYear() + "-" + (now.getMonth() + 1) + "-" + now.getDate() + " " + timeString;
            window.setTimeout("tick();", 1000);
        }

        //load事件
        function check() {
            //获得系统当前时间的方法
            tick();
        }
    </script>
</head>
<body>
    <div id="app">
        @{Engage_Major_Release eng = ViewBag.s;}
        @using (Html.BeginForm("Show_UPDATE", "ZhiWeiFaBu", FormMethod.Post))
        {
            <form role="form" action="/ZhiWeiFaBu/Show_UPDATE" method="post">

                <table width="100%">
                    <tr>
                        <td>
                            <font color="black">您正在做的业务是：人力资源--招聘管理--职位发布管理--职位发布登记 </font>
                        </td>
                    </tr>
                    <tr>
                        <td align="right">
                            <input type="submit" value="提交" class="BUTTON_STYLE1">
                            <input type="reset" value="清除" class="BUTTON_STYLE1">
                        </td>

                    </tr>
                </table>
                <table width="100%" border="1" cellpadding=0 cellspacing=1
                       bordercolorlight=#848284 bordercolordark=#eeeeee
                       class="TABLE_STYLE1">
                    <tr>
                        <td class="TD_STYLE1" width="11%">
                            I级
                        </td>
                        <td class="TD_STYLE1" width="11%" readonly="readonly">
                            <input readonly="readonly" type="hidden" class="INPUT_STYLE2" value=" @eng.mre_id" name="mre_id" id="mre_id">
                            <input readonly="readonly" type="text" class="INPUT_STYLE2" value=" @eng.first_kind_name" name="first_kind_name" id="first_kind_name">
                        </td>
                        <td class="TD_STYLE1" width="11%">
                            II级
                        </td>
                        <td class="TD_STYLE1" width="11%">
                            <input readonly="readonly" type="hidden" class="INPUT_STYLE2" value=" @eng.second_kind_id" name="second_kind_id" id="second_kind_id">
                            <input readonly="readonly" type="text" class="INPUT_STYLE2" value=" @eng.second_kind_name" name="second_kind_name" id="second_kind_name">
                        </td>
                        <td class="TD_STYLE1" width="11%">
                            III级
                        </td>
                        <td class="TD_STYLE1" width="11%">
                            <input readonly="readonly" type="hidden" class="INPUT_STYLE2" value=" @eng.third_kind_id" name="third_kind_id" id="third_kind_id">
                            <input readonly="readonly" type="text" class="INPUT_STYLE2" value=" @eng.third_kind_name" name="third_kind_name" id="third_kind_name">
                        </td>
                        <td width="11%" class="TD_STYLE1">
                            招聘类型
                        </td>

                        <td class="TD_STYLE2" colspan="2">
                            <select value="@eng.engage_type" name="engage_type" id="engage_type" readonly>
                                <option>校园招聘</option>
                                <option>社会招聘</option>
                            </select>
                        </td>
                    </tr>
                    <tr>
                        <td class="TD_STYLE1">
                            职位分类
                        </td>
                        <td class="TD_STYLE2">
                            <input readonly="readonly" type="hidden" class="INPUT_STYLE2" value=" @eng.major_kind_id" name="major_kind_id" id="major_kind_id">
                            <input readonly="readonly" type="text" class="INPUT_STYLE2" value=" @eng.major_kind_name" name="major_kind_name" id="major_kind_name">
                        </td>
                        <td class="TD_STYLE1">
                            职位名称
                        </td>
                        <td class="TD_STYLE2">
                            <input readonly="readonly" type="hidden" class="INPUT_STYLE2" value=" @eng.major_id" name="major_id" id="major_id">
                            <input readonly="readonly" type="text" class="INPUT_STYLE2" value=" @eng.major_name" name="major_name" id="major_name">
                        </td>

                        <td class="TD_STYLE1">
                            招聘人数
                        </td>
                        <td class="TD_STYLE2">
                            <input type="text" class="INPUT_STYLE2" value=" @eng.human_amount" name="human_amount" id="human_amount">
                        </td>
                        <td class="TD_STYLE1">
                            截止日期
                        </td>
                        <td class="TD_STYLE2">
                            <input type="text" class="INPUT_STYLE2" value=" @eng.deadline" name="deadline" id="deadline">

                        </td>

                    </tr>

                    <tr>
                        <td class="TD_STYLE1">
                            变更人
                        </td>
                        <td class="TD_STYLE2">
                            <input type="text" class="INPUT_STYLE2" value=" @eng.changer" placeholder="请输入名称" name="changer" id="changer">
                        </td>
                        <td class="TD_STYLE1">
                            变更时间
                        </td>
                        <td class="TD_STYLE2">
                            <input readonly="readonly" type="text" class="INPUT_STYLE2" value=" @eng.change_time" id="nowTime" name="change_time">
                        </td>
                        <td class="TD_STYLE2">&nbsp; </td>
                        <td class="TD_STYLE2">&nbsp; </td>
                        <td class="TD_STYLE2">&nbsp; </td>
                        <td class="TD_STYLE2">&nbsp; </td>

                    </tr>
                    <tr>
                        <td class="TD_STYLE1">
                            职位描述
                        </td>
                        <td class="TD_STYLE2" colspan="8">
                            
                            <input type="text" class="INPUT_STYLE2" value=" @eng.major_describe" id="human_amount" name="human_amount">
                        </td>
                    </tr>
                    <tr>
                        <td class="TD_STYLE1">
                            招聘要求
                        </td>
                        <td class="TD_STYLE2" colspan="8">
                            <input type="text" class="INPUT_STYLE2" value=" @eng.human_amount" id="human_amount" name="human_amount">
                        </td>

                    </tr>

                </table>
            </form>
        }
    </div>

    <script type="text/javascript">
        var vm = new Vue({
            el: "#app",
            data() {
                return {
                    tableData: [],//进行存储一级数据
                    form: "",//进行存储一级id
                    table2: [],//进行存储二级数据
                    form2: "",//进行存储二级id
                    table3: [],//进行存储三级数据
                    form3: "",//进行存储三级id

                    region: "",//进行存储是否
                    mc: "",
                    fu: "",
                    options: [],
                    value: [],

                }
            },
            methods: {

            },
            mounted: function () {

            }
        });
    </script>
</body>
</html>
